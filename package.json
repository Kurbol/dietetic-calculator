{
  "name": "dietetic-calculator",
  "version": "1.0.0",
  "description": "Tool For Common Dietetic Calculations",
  "author": "Curtis Bollinger",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "test": "ava",
    "docker-build": "docker build -t dietetic-calculator-web .",
    "docker-tag": "docker tag dietetic-calculator-web gcr.io/dietetic-calculator/dietetic-calculator-web:latest",
    "docker-push": "docker push gcr.io/dietetic-calculator/dietetic-calculator-web:latest",
    "docker-run": "docker run --name dietetic-calculator-web --env-file .env -p 3000:3000 -d dietetic-calculator-web",
    "deploy": "docker-build && docker-tag && docker-push"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@nuxtjs/pwa": "^3.0.0-beta.19",
    "graphql-request": "^1.8.2",
    "nuxt": "^2.10.1",
    "ramda": "^0.26.1"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^1.1.2",
    "@nuxtjs/eslint-module": "^1.1.0",
    "@nuxtjs/vuetify": "^1.9.0",
    "@vue/test-utils": "^1.0.0-beta.29",
    "ava": "^2.4.0",
    "babel-eslint": "^10.0.3",
    "babel-plugin-module-resolver": "^3.2.0",
    "browser-env": "^3.2.6",
    "dotenv": "^8.2.0",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-nuxt": "^0.4.3",
    "eslint-plugin-prettier": "^3.1.1",
    "husky": "^3.0.9",
    "lint-staged": "^9.4.2",
    "node-sass": "^4.12.0",
    "prettier": "^1.18.2",
    "require-extension-hooks": "^0.3.3",
    "require-extension-hooks-babel": "^1.0.0",
    "require-extension-hooks-vue": "^3.0.0"
  }
}
