{
  "name": "dietetic-calculator",
  "version": "1.0.0",
  "description": "Tool For Common Dietetic Calculations",
  "author": "Curtis Bollinger",
  "scripts": {
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "test": "ava --serial --verbose",
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start && npm run db",
    "generate": "nuxt generate",
    "db": "json-server --watch db.json --port 4000 --read-only",
    "db-delay": "json-server -d 1500 --watch db.json --port 4000",
    "docker-build": "docker build -t dietetic-calculator-web .",
    "docker-run": "docker run -p 3000:3000 dietetic-calculator-web",
    "docker-tag": "docker tag dietetic-calculator-web gcr.io/dietetic-calculator/dietetic-calculator-web:latest",
    "docker-push": "docker push gcr.io/dietetic-calculator/dietetic-calculator-web:latest",
    "deploy": "docker-build & docker-tag & docker-push"
  },
  "dependencies": {
    "@nuxtjs/pwa": "^3.0.0-beta.19",
    "graphql-request": "^1.8.2",
    "lodash": "^4.17.15",
    "nuxt": "^2.9.2",
    "ramda": "^0.26.1"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^1.1.2",
    "@nuxtjs/eslint-module": "^1.1.0",
    "@nuxtjs/vuetify": "^1.8.4",
    "@vue/test-utils": "^1.0.0-beta.29",
    "ava": "^2.4.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "eslint": "^5.0.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-nuxt": "^0.4.3",
    "eslint-plugin-prettier": "^3.1.0",
    "jest": "^24.9.0",
    "jsdom": "^15.1.1",
    "node-sass": "^4.12.0",
    "prettier": "^1.18.2",
    "vue-jest": "^3.0.5"
  }
}
